# Environment variables template
# Copy to .env and fill in values

# Database
DATABASE_URL=postgresql+asyncpg://cattle:cattle_password@localhost:5432/cattle_diagnosis

# Gemini API
GEMINI_API_KEY=your_gemini_api_key_here

# Application
APP_NAME=Cattle Disease Diagnosis Platform
DEBUG=true
API_V1_PREFIX=/api

# Storage (S3-compatible)
STORAGE_ENDPOINT=
STORAGE_ACCESS_KEY=
STORAGE_SECRET_KEY=
STORAGE_BUCKET=cattle-disease-media
